{% extends 'base.html.twig' %}

{% block title %}New Answers{% endblock %}

{% block body %}
    <h1>Create new Answers</h1>

    {{ include('answers/_form.html.twig') }}

    <a href="{{ path('answers_index') }}">back to list</a>


    {% for question in answers[oneTen] %}
        <tr>
            <td>{{ loop.index }}</td>
            <td>{{ question.id }} ?</td>

        </tr>
    {% endfor %}


{% endblock %}
